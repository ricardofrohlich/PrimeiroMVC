@{
    ViewData["Title"] = "Mostrar mensagem";
}
<h2> Oi gente, estou na página do OláMundo/MostrarMensagem</h2>

<h1>@ViewData["nome"]</h1>
<h2>@ViewData["Qtd"]</h2>

<ul>
@for(int i=0;i< (int)ViewData["Qtd"]; i++)
{
    <li>@ViewData["nome"]</li>
}
</ul>

